{
  "manifest_version": 2,

  "name": "BM auth",
  "description": "This extension getBMHPS FROM BmNotifier and put in session",
  "version": "1.0",
  "background": {"scripts": ["background.js"]},
  "browser_action": {
    "default_icon": "bm.png"
    
  },
  "permissions": [
	"webRequest", "webRequestBlocking",
	"http://localhost:51985/*",
	"*://*/*",
	"cookies",
	"tabs"
	
  ]
}
